&#xD;
                    await _unitOfWork.CommitAsync();&#xD;
&#xD;
                    response = BaseDataResponse&lt;$VIEWMODEL$&gt;.Success(_mapper.Map&lt;$VIEWMODEL$&gt;($VARENTITY$));&#xD;
                }&#xD;
            }&#xD;
&#xD;
            return response;&#xD;
        }&#xD;
    }&#xD;
}&#xD;
